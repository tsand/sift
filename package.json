{
  "name": "sift-css",
  "version": "0.2.3",
  "description": "Automatically remove unused CSS rules using Chrome code coverage",
  "files": [
    "dist/**/*"
  ],
  "bin": {
    "sift-css": "dist/bin/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "format": "eslint src/** --fix",
    "lint": "eslint src/**",
    "test": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine.json"
  },
  "dependencies": {
    "postcss": "^8.2.9",
    "puppeteer": "^8.0.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/jasmine": "^3.6.9",
    "@types/node": "^14.14.37",
    "@types/puppeteer": "^5.4.3",
    "@types/yargs": "^16.0.1",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "devtools-protocol": "0.0.868034",
    "eslint": "^7.23.0",
    "jasmine": "^3.7.0",
    "ts-loader": "^8.1.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.3"
  },
  "author": "Theisen Sanders",
  "license": "MIT",
  "homepage": "https://github.com/tsand/sift-css#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsand/sift-css.git"
  },
  "keywords": [
    "sift",
    "puppeteer",
    "css",
    "coverage",
    "cli"
  ],
  "bugs": {
    "url": "https://github.com/tsand/sift-css/issues"
  }
}
