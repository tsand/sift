{
  "name": "sift-css",
  "version": "0.2.5",
  "description": "Automatically remove unused CSS rules using Chrome code coverage",
  "files": [
    "dist/**/*"
  ],
  "bin": {
    "sift-css": "dist/bin/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "format": "eslint src/** --fix",
    "lint": "eslint src/**",
    "test": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine.json"
  },
  "dependencies": {
    "postcss": "^8.4.4",
    "puppeteer": "^13.1.3",
    "yargs": "^17.3.0"
  },
  "devDependencies": {
    "@types/jasmine": "^3.10.2",
    "@types/node": "^16.11.11",
    "@types/puppeteer": "^5.4.4",
    "@types/yargs": "^17.0.7",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "devtools-protocol": "0.0.982423",
    "eslint": "^8.3.0",
    "jasmine": "^3.10.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "author": "Theisen Sanders",
  "license": "MIT",
  "homepage": "https://github.com/tsand/sift-css#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsand/sift-css.git"
  },
  "keywords": [
    "sift",
    "puppeteer",
    "css",
    "coverage",
    "cli"
  ],
  "bugs": {
    "url": "https://github.com/tsand/sift-css/issues"
  }
}
